      .row
        h2 Temperature Control
      .row
        .col-lg-12.col-md-12
          h3.grey
            | RIMS | Setpoint: 
            span.must.sliderLabel(data-name="rimsTemp", data-device-type="tempController", data-device-name="rims", data-device-arg="setpoint", data-output-type="html") 100
            span.pull-right(data-device-type="tempController", data-device-name="rims", data-device-arg="duration", data-output-type="html") 00:00:00
            span.pull-right Duration:
          .slider.sliderMin(data-name="rimsTemp", data-default-value="152", data-min="80", data-max="180", data-device-type="tempController", data-device-name="rims", data-device-arg="setpoint", data-output-type="slider")
          // .tempFlotChart(data-device-name="rims", data-device-type="tempSensor", data-device-arg="chartData", data-output-type="tempFlotChart")
          button.btn.btn-primary.btn-wide.btnOnOff(type="button", data-device-type="tempController", data-device-name="rims", data-device-arg="state", data-output-type="button", data-device-state="true")
            | RIMS Temp Control
      .row
        h2 Pump Control
      .row
        .col-lg-3.col-sm-6.col-xs-6.col-xxs-12
          .smallstat.box
            .box-header
